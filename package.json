{
    "name": "plaintext-syntax-highlighting",
    "displayName": "Plaintext Syntax Highlighting",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "plaintext",
            "aliases": ["Plaintext", "plaintext"],
            "extensions": [".txt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "plaintext",
            "scopeName": "source.txt",
            "path": "./syntaxes/plaintext.tmLanguage.json"
        }]
    }
}